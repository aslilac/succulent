{
	"name": "succulent",
	"author": "McKayla Washburn <mckayla@hey.com>",
	"license": "MIT",
	"version": "0.19.0",
	"description": "Powerful and easy runtime type checking",
	"repository": "https://github.com/aslilac/succulent",
	"keywords": [
		"typescript",
		"type checking",
		"validation",
		"types",
		"type safety",
		"runtypes",
		"yup",
		"zod"
	],
	"type": "module",
	"module": "./build/index.js",
	"types": "./build/index.d.ts",
	"exports": {
		".": {
			"import": "./build/index.js"
		}
	},
	"files": [
		"build/**"
	],
	"devDependencies": {
		"@mckayla/prettier": "^1.0.2",
		"@mckayla/tsconfig": "^1.0.2",
		"@types/jest": "^29.1.1",
		"eslint": "^8.24.0",
		"eslint-config-mckayla": "^1.0.4",
		"jest": "^29.1.2",
		"prettier": "^2.7.1",
		"ts-jest": "^29.0.3",
		"typescript": "^4.8.4"
	},
	"scripts": {
		"build": "tsc -b tsconfig.build.json",
		"fmt": "prettier --write .",
		"lint": "eslint .",
		"prepack": "npm run build -s",
		"test": "jest"
	}
}
